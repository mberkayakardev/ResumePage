<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="author" content="Berkay AKAR" />
    <meta name="description" content="Berkay Akar Kişisel Portfolio" />
    <link rel="apple-touch-icon" href="~/Theme/Cv/assets/images/icon.png">
    <link rel="shortcut icon" href="~/Theme/Cv/assets/images/icon.png" type="image/png">
    <link href="~/Theme/Admin/codervent.com/syntrans/demo/vertical/assets/plugins/simplebar/css/simplebar.css" rel="stylesheet" />
    <link href="~/Theme/Admin/codervent.com/syntrans/demo/vertical/assets/plugins/perfect-scrollbar/css/perfect-scrollbar.css" rel="stylesheet" />
    <link href="~/Theme/Admin/codervent.com/syntrans/demo/vertical/assets/plugins/metismenu/css/metisMenu.min.css" rel="stylesheet" />
    <link href="~/Theme/Admin/codervent.com/syntrans/demo/vertical/assets/css/pace.min.css" rel="stylesheet" />
    @* <script src="~/Theme/Admin/codervent.com/syntrans/demo/vertical/assets/js/pace.min.js"></script> *@
    <link rel="stylesheet" href="~/Theme/Admin/codervent.com/syntrans/demo/vertical/assets/css/bootstrap.min.css" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@300;400;600&amp;family=Roboto&amp;display=swap" />
    <link rel="stylesheet" href="~/Theme/Admin/codervent.com/syntrans/demo/vertical/assets/css/icons.css" />
    <link rel="stylesheet" href="~/Theme/Admin/codervent.com/syntrans/demo/vertical/assets/css/app.css" />
     
    @RenderSection("style", false)
</head>

<body class="bg-theme bg-theme1">
    <!-- wrapper -->
    <div class="wrapper">

        <partial name="PartialViews/_Navbar" />

        <!--page-wrapper-->
        <div class="page-wrapper">
            <!--page-content-wrapper-->
            <div class="page-content-wrapper">
                <div class="page-content">
                    <!--end breadcrumb-->

                    <div class="row">
                        @RenderBody()
                    </div>
                </div>
            </div>
            <!--end page-content-wrapper-->
        </div>
        <!--end page-wrapper-->
        <!--start overlay-->
        <div class="overlay toggle-btn-mobile"></div>
        <!--end overlay-->
        <!--Start Back To Top Button--> <a href="javaScript:;" class="back-to-top"><i class='bx bxs-up-arrow-alt'></i></a>
        <!--End Back To Top Button-->

    </div>
    <!-- end wrapper -->
    <!--start switcher-->

    <div class="switcher-wrapper">
        <div class="switcher-btn">
            <i class='bx bx-cog bx-spin'></i>
        </div>
        <div class="switcher-body">
            <h5 class="mb-0 text-uppercase">Theme Customizer</h5>
            <hr />
            <p class="mb-0">Gaussian Texture</p>
            <hr>

            <ul class="switcher">
                <li id="theme1"></li>
                <li id="theme2"></li>
                <li id="theme3"></li>
                <li id="theme4"></li>
                <li id="theme5"></li>
                <li id="theme6"></li>
            </ul>
            <hr>
            <p class="mb-0">Gradient Background</p>
            <hr>

            <ul class="switcher">
                <li id="theme7"></li>
                <li id="theme8"></li>
                <li id="theme9"></li>
                <li id="theme10"></li>
                <li id="theme11"></li>
                <li id="theme12"></li>
            </ul>
        </div>
    </div>



    <script src="~/Theme/Admin/codervent.com/syntrans/demo/vertical/assets/js/jquery.min.js"></script>
    <script src="~/Theme/Admin/codervent.com/syntrans/demo/vertical/assets/js/popper.min.js"></script>
    <script src="~/Theme/Admin/codervent.com/syntrans/demo/vertical/assets/js/bootstrap.min.js"></script>
    <script src="~/Theme/Admin/codervent.com/syntrans/demo/vertical/assets/plugins/simplebar/js/simplebar.min.js"></script>
    <script src="~/Theme/Admin/codervent.com/syntrans/demo/vertical/assets/plugins/metismenu/js/metisMenu.min.js"></script>
    <script src="~/Theme/Admin/codervent.com/syntrans/demo/vertical/assets/plugins/perfect-scrollbar/js/perfect-scrollbar.js"></script>
    <script src="~/Theme/Admin/codervent.com/syntrans/demo/vertical/assets/js/app.js"></script>
     
    @RenderSection("script", false)
    @await Component.InvokeAsync("NToastNotify")

</body>
</html>

